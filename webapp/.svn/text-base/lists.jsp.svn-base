<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="keywords" content="" />
<meta name="description" content="" />
<link href="<%=basePath %>css/common.css" rel="stylesheet" />
<link href="<%=basePath %>css/index.css" rel="stylesheet" type="text/css" />
<link href="<%=basePath %>css/detail.css" rel="stylesheet" type="text/css"/>
<script type="text/javascript" src="<%=basePath %>script/jquery.min.js"></script>
<script type="text/javascript" src="<%=basePath %>script/common.js"></script>
<script type="text/javascript" src="<%=basePath%>jq/jquery.js"></script>
<title>Insert title here</title>

<script type="text/javascript">
	$(window.parent.document).find("#ifra").load(function () {   
		var main = $(window.parent.document).find("#ifra");   
		var thisheight = $(document).height() + 30;    
			main.height(thisheight);
		}); 


</script>
</head>
<body>
		
	        <div class="bd">
      		<div class="invest-table clearfix" >
        		<div class="title clearfix">
         		 <ul>
           		 	<li class="col-330">理财产品标题</li>
            	 	<li class="col-180"><a href="javascript:url('order','account_up');" class="">产品总额</a> </li>
           			<li class="col-180"><a href="javascript:url('order','apr_up');" class="">最低投资金额</a> </li>
            		<li class="col-150"><a href="javascript:url('order','period_up');" class="">理财产品年利率</a> </li>
            		<li class="col-120"><a href="javascript:url('order','scale_up');" class="">满标进度</a></li>
            		<li class="col-120-t" style="text-align:center">操作</li>
          		</ul>
        	   </div>
        		<!------------投资列表-------------->
        		<div class="item" id="probox">
        			<div class="bd">
	      				<c:forEach var="pro" items="${pro }">
	      					<ul><li class="col-330 col-t">
				    				<a href="infor.jsp" target="_blank"><i class="icon icon-xin" title="类型"></i></a>
				    				<a class="f18" href="infor.jsp" title="标题" target="_blank">${pro.proname }</a></li>	<!-- 标题 -->
								<li class="col-180"><span class="f20 c-333">${pro.promoney }</span>元</li>		<!-- 产品总额 -->
								<li class="col-180 relative"><span class="f20 c-333">${pro.proprice }元</span></li>		<!-- 最低投资金额 -->
								<li class="col-150"><span class="f20 c-orange">${pro.proprofit }%</span></li>	<!-- 理财产品年利率 -->
								<li class="col-120">
									<div class="circle">
										<div class="left progress-bar">
				     						<div class="progress-bgPic progress-bfb">
				     							<div class="show-bar">30.00% </div>
				     						</div>
				     					</div>
									</div>
								</li>
				 				<li class="col-120-2">	 <!-- 募集完成        还款中 -->
				     				<a class="ui-btn btn-gray"  href="<%=basePath%>/pro/infor?proid=${pro.proid }" target="blank">立即投标</a>
				 				</li>
		  					</ul>
        				 </c:forEach>
         </div>
        		</div>
      		</div>
      
    	</div>
    	
    	            <div class="pagination clearfix mrt30"><!-- 分页开始 -->
      	<span class="page">
      
      	<c:if test="${mark==1 }">
		  <c:choose>
			<c:when test="${pro!=null }">
			
				<a href="<%=basePath%>pro/prolist/${p.getFirstPage()}"
					class="btn btn-success btn-ms ">首页</a> 
				<c:if test="${page>1}">
				<a href="<%=basePath%>pro/prolist/${page-1}"
					class="btn btn-success btn-ms ">&lt;&lt;上一页</a> 
				</c:if>
				<c:if test="${page<p.getLastPage()}">
				<a href="<%=basePath%>pro/prolist/${page+1}"
					class="btn btn-success btn-ms "> 下一页&gt;&gt;</a>
				</c:if>
				<a href="<%=basePath%>pro/prolist/${p.getLastPage()}"
					class="btn btn-success btn-ms "> 尾页</a>
				<font size="3">共<font color="red">${p.getTotal()}</font>条数据，当前<font color="#0000ff">${p.getPageNum()}</font><font
					color="red">/${p.getLastPage()}</font>页
				</font>
			</c:when>
			<c:otherwise></c:otherwise>
		</c:choose>
		</c:if>
		
		<c:if test="${mark==2 }">
			<c:choose>
				<c:when test="${pro!=null }">
					<a href="<%=basePath%>pro/prosltlist/${p.getFirstPage()}/${opt }/${val}"
						class="btn btn-success btn-ms ">首页</a>
					<c:if test="${page>1}">
					<a href="<%=basePath%>pro/prosltlist/${page-1}/${opt }/${val}"
						class="btn btn-success btn-ms ">&lt;&lt;上一页</a>
					</c:if>
					<c:if test="${page<p.getLastPage()}">
					<a href="<%=basePath%>pro/prosltlist/${page+1}/${opt }/${val}"
						class="btn btn-success btn-ms "> 下一页&gt;&gt;</a>
					</c:if>
					<a href="<%=basePath%>pro/prosltlist/${p.getLastPage()}/${opt }/${val}"
						class="btn btn-success btn-ms "> 尾页</a>
					<font size="3">共<font color="red">${p.getTotal()}</font>条数据，当前<font color="#0000ff">${p.getPageNum()}</font><font
						color="red">/${p.getLastPage()}</font>页
					</font>
				</c:when>
			<c:otherwise>
			</c:otherwise>
			</c:choose>
		</c:if>

		<c:if test="${mark==3 }">
			<c:choose>
				<c:when test="${pro!=null }">
					<a href="<%=basePath%>pro/protypex/${p.getFirstPage()}?protype=${type }"
						class="btn btn-success btn-ms ">首页</a> 
					<c:if test="${page>1}">
					<a href="<%=basePath%>pro/protypex/${page-1}?protype=${type }"
						class="btn btn-success btn-ms ">&lt;&lt;上一页</a>
					</c:if>
					<c:if test="${page<p.getLastPage()}">
					<a href="<%=basePath%>pro/protypex/${page+1}?protype=${type }"
						class="btn btn-success btn-ms "> 下一页&gt;&gt;</a>
					</c:if>
					<a href="<%=basePath%>pro/protypex/${p.getLastPage()}?protype=${type }"
						class="btn btn-success btn-ms "> 尾页</a>
					<font size="3">共<font color="red">${p.getTotal()}</font>条数据，当前<font color="#0000ff">${p.getPageNum()}</font><font
						color="red">/${p.getLastPage()}</font>页
					</font>
				</c:when>
			<c:otherwise>
			</c:otherwise>
			</c:choose>
		</c:if>
      
      </span>
      </div><!-- 分页结束 -->
</body>
</html>