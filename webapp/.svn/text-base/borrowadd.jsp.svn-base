<script type="text/javascript">
	
	//身份证失去焦点即调用接口查询该号码对应的个人信息
 	function addLocAddress(){
		var idNum=$("#useridcard").val();
		$.ajax({
			url:"/rent/addLocAddress",
			data:{"idNum":idNum},
			type:"post",
			dataType:"json",
			success:function(data){
				var msgcode=data.resultcode;
				if(msgcode!=200){
					$("#tishi").html(data.error).css("color","red");
				}else{
					$("#tishi").html("身份证正确").css("color","green");
				}
				var area=data.area;
				$("#useraddress").val(area);
			}
		})			
	} 
	//验证姓名信息
	var nameYz=false;
	function nameyz(){
		nameYz=false;
		if($("#username").val()==null || $("#username").val()==""){
			$("#namets").html("用户名不能为空").css("color","red");
			namYz=false;
		}else{
			$("#namets").html("输入正确").css("color","green");
			nameYz=true;
		}
	}
	//验证生日不为空
	var srYz=false;
	function sryz(){
		srYz=false;
		if($("#userdate").val()==null || $("#userdate").val()==""){
			$("#srts").html("生日不能为空").css("color","red");
			srYz=false;
		}else{
			$("#srts").html("输入正确").css("color","green");
			srYz=true;
		}
	}
	//验证手机号码
	var numYz=false;
	function numyz(){
		numYz=false;
		if($("#usertel").val()==null || $("#usertel").val()==""){
			$("#numts").html("手机号码不能为空").css("color","red");
			numYz=false;
		}else{
			var re=/^(13[0-9]{9})|(15[0-9]{9})|^(17[0-9]{9})|^(18[0-9]{9})$/;   
			if(!re.test($("#usertel").val())){
				$("#numts").html("请输入正确的手机号码").css("color","red");
				numYz=false;
			}else{
				$("#numts").html("输入正确").css("color","green");
				numYz=true;
			}
		}
	}
 
	//验证贷款金额
	var rentYz=false;
	function rentyz(){
		rentYz=false;
		if($("#userrent").val()==null || $("#userrent").val()==""){
			$("#rentts").html("贷款金额不能为空").css("color","red");
			rentYz=false;
		}else{			
			$("#rentts").html("输入正确").css("color","green");
			rentYz=true;		
		}
	}
	//验证贷款用途
	var rentforYz=false;
	function rentforyz(){
		rentforYz=false;
		if($("#userrentfor").val()==null || $("#userrentfor").val()==""){
			$("#rentforts").html("用途不能为空").css("color","red");
			rentforYz=false;
		}else{			
			$("#rentforts").html("输入正确").css("color","green");
			rentforYz=true;		
		}
	}
	//提交表单前验证表单信息
	function yzForm(){
		var simgYz=false;
		var bankimgYz=false;
		if($("#useridcardpath").val().length>0){
			simgYz=true;
		}else{
			$("#simgts").html("必须上传身份证图片").css("color","red");
			simgYz=false;
		}
		if($("#usermoneypath").val().length>0){
			bankimgYz=true;
		}else{
			$("#bankimgts").html("银行流水必须上传").css("color","red");
			bankimgYz=false;
		}
		if(nameYz  && numYz && simgYz && bankimgYz && rentYz && rentforYz){
			$("#tjts").html("提交成功").css("green","red");
			$("#form").submit();
		}else{
			$("#tjts").html("提交失败").css("color","red");
		}
	} 
</script>
 
 
	 
