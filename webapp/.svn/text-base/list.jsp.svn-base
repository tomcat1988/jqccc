<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>富二贷P2P平台</title>
<meta name="keywords" content="" />
<meta name="description" content="" />
<link href="<%=basePath %>css/common.css" rel="stylesheet" />
<link href="<%=basePath %>css/index.css" rel="stylesheet" type="text/css" />
<link href="<%=basePath %>css/detail.css" rel="stylesheet" type="text/css"/>
<script type="text/javascript" src="<%=basePath %>script/jquery.min.js"></script>
<script type="text/javascript" src="<%=basePath %>script/common.js"></script>
<script type="text/javascript" src="<%=basePath%>jq/jquery.js"></script>

<script type="text/javascript">
	

</script>

<script type="text/javascript">
	
	$("#ifra").load(function () {
		var mainheight = $(this).contents().find("body").height() + 30; 
		$(this).height(mainheight); }); 
	
	$(function(){
		var protype="";
		var fiit="";
		var proday="";
		var proprice="";
		$(document).ready(function(){
			$(".type").change(function(){
				protype=$(this).val();
				$("#ifra").attr("src","<%=basePath%>/pro/prosltlist/1/1/"+protype);
				
			});			
			$(".profit").change(function(){
				fiit=$(this).val();
				$("#ifra").attr("src","<%=basePath%>/pro/prosltlist/1/2/"+fiit);
				
			});
			$(".day").change(function(){
				proday=$(this).val();
				$("#ifra").attr("src","<%=basePath%>/pro/prosltlist/1/3/"+proday);
				
			});
			$(".price").change(function(){
				proprice=$(this).val();
				$("#ifra").attr("src","<%=basePath%>/pro/prosltlist/1/4/"+proprice);
				
				                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
			});
		});
	});
	
	$(function (){
		var tips=$("#tips").val();
		if(tips=="4"){
			$("#ifra").attr("src","<%=basePath%>/pro/prolist/1");
		}else{
			$("#ifra").attr("src","<%=basePath %>/pro/protypex/1?protype="+tips);
		}
	});
</script>
</head>
<body>
  <!-- 导航栏 --><jsp:include page="head.jsp"></jsp:include>
  
<!--列表-->
<div class="page-filter wrap">
  <div class="breadcrumbs"><a href="index.jsp">首页</a>&gt;<span class="cur">理财投资列表</span></div>
  
  <input type="hidden" value="${tipp }" id="tips"/>
  
  <div class="invest-filter" data-target="sideMenu">
    <div class="filter-inner clearfix">
    
    
      <div class="filter-item">
        <div class="hd">
          <h3>筛选投资项目</h3>
        </div>
        <form action="" method="post">
        <div class="bd" id="kkk"> <!-- 通过条件显示给用户的展示理财产品信息 -->
          
        <dl>
            <dt>产品类型</dt>
            <dd>
              <ul>
                <li class="n1"><input type="radio" name="protype" value="0" class="type" target="ifbox" checked="checked"/>不限</li>
                <li class="n2"><input type="radio" name="protype" value="1" class="type" target="ifbox"/>小金金</li>
                <li class="n3"><input type="radio" name="protype" value="2" class="type" target="ifbox"/>小券券</li>
                <li class="n4"><input type="radio" name="protype" value="3" class="type" target="ifbox"/>小票票</li>
              </ul>
            </dd>
        </dl> 
          
          
       <dl>
          <dt>产品收益率</dt>
          <dd>
          	<ul>
            	<li class="n1"><input type="radio" name="proprofit" value="0" target="ifbox" class="profit" checked="checked" />不限</li>
                <li class="n2"><input type="radio" name="proprofit" value="5.00"  target="ifbox" class="profit"/>5%以下</li>
                <li class="n3"><input type="radio" name="proprofit" value="10.00" target="ifbox" class="profit"/>5%-10%</li>
                <li class="n4"><input type="radio" name="proprofit" value="15.00" target="ifbox" class="profit"/>10%-20%</li>
                <li class="n5"><input type="radio" name="proprofit" value="20.00" target="ifbox" class="profit"/>20%及以上</li>
            </ul>
          </dd>
       </dl>
          
          
          <dl>
            <dt>产品投标期限</dt>
            <dd> 
              <ul>
                <li class="n1"><input type="radio" name="proday" value="0" target="ifbox" class="day" checked="checked"/>不限 </li>
                <li class="n2"><input type="radio" name="proday" value="3"  target="ifbox" class="day"/>3个月</li>
                <li class="n3"><input type="radio" name="proday" value="6"  target="ifbox" class="day"/>6个月</li>
                <li class="n5"><input type="radio" name="proday" value="12" target="ifbox" class="day"/>12月及以上</li>
              </ul>
            </dd>
          </dl> 
          
         <dl>
            <dt>最低投标金额</dt>
            <dd>
              <ul>
              	<li class="n1"><input type="radio" name="proprice" value="0" target="ifbox" class="price" checked="checked"/>不限 </li>
                <li class="n2"><input type="radio" name="proprice" value="100"  class="price"  target="ifbox"/>100</li>
                <li class="n3"><input type="radio" name="proprice" value="200"  class="price"  target="ifbox"/>200</li>
                <li class="n4"><input type="radio" name="proprice" value="500"  class="price"  target="ifbox"/>500</li>
                <li class="n5"><input type="radio" name="proprice" value="1000" class="price"  target="ifbox"/>1000</li>
              </ul>
            </dd>
          </dl> 
         
        </div>
        <div class="bd">
        </div>
         </form>
      </div>
      
 	 <div class="common-problem">
        <h3>常见问题</h3>
        <ul>
          <li><a href="#">什么是P2P理财?</a></li>
          <li><a href="#">收益规则时如何计算的？</a></li>
          <li><a href="#">富二贷网贷P2P理财收费标准</a></li>
          <li><a href="#">小金金和小券券、小票票有什么区别？</a></li>
        </ul>
      </div>
     
      
    </div>
  </div>
  
  <div class="invest-list mrt30 clearfix">
    <iframe name="ifbox" width="100%" height="100%" class="item" src="<%=basePath%>pro/prolist/1" 
        id="ifra" frameborder="0" scrolling="auto" style="overflow: visible;" frameborder="0">
        		

     </iframe>

  </div>
</div>
<!-- 导航栏 --><jsp:include page="bottom.jsp"></jsp:include>

</body>
</html>
